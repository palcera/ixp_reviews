{
    "name": "palcera/ixp-review",
    "description": "This is the site to review the Drupal IXP initiative process",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://drupal-ixp.org",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/allowed_formats": "^3.0",
        "drupal/bootstrap5": "^4.0",
        "drupal/bootstrap_layout_builder": "^2.2",
        "drupal/bpmn_io": "^2.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_overlay": "^2.3",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/custom_field": "^3.0",
        "drupal/devel": "^5.1",
        "drupal/dropdown_language": "^4.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/eca": "^2.1",
        "drupal/eca_tamper": "^2.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/editoria11y": "^2.1",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0",
        "drupal/gin": "^3.0@rc",
        "drupal/gin_toolbar": "^1.0@rc",
        "drupal/google_tag": "^2.0",
        "drupal/group": "^3.3",
        "drupal/group_action": "^1.2",
        "drupal/http_cache_control": "^2.1",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/layout_builder_blocks": "^1.1",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "^7.0@alpha",
        "drupal/login_redirect_per_role": "^1.10",
        "drupal/mailsystem": "^4.5",
        "drupal/manage_display": "3.x-dev@dev",
        "drupal/markdown_easy": "^1.0",
        "drupal/metatag": "^2.0",
        "drupal/moderation_dashboard": "^3.1",
        "drupal/moderation_sidebar": "^1.7",
        "drupal/pathauto": "^1.11",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/profile": "^1.11",
        "drupal/radix": "~6",
        "drupal/redirect": "^1.9",
        "drupal/redirect_404": "^1.9",
        "drupal/robotstxt": "^1.6",
        "drupal/schema_metatag": "^3.0",
        "drupal/search404": "^2.2@RC",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^3",
        "drupal/select2": "^2",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/sitemap": "^2.0",
        "drupal/smart_date": "^4.2",
        "drupal/stage_file_proxy": "^3.1",
        "drupal/upgrade_status": "^4.3",
        "drupal/user_current_paths": "^2.0",
        "drush/drush": "^13",
        "mglaman/composer-drupal-lenient": "^1.0",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "mglaman/composer-drupal-lenient": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "bower-asset",
                "npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/group": {
                "Issue 3494447: Undefined function field_ui_entity_operation": "https://git.drupalcode.org/project/group/-/merge_requests/204.patch"
            },
            "drupal/entity_reference_views" : {
                "Issue 3514454: The Select2 requirement is not compatible with Drupal 11": "https://git.drupalcode.org/project/entity_reference_views/-/merge_requests/8.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": [
              "drupal/entity_reference_views"
            ]
        }
    }
}
